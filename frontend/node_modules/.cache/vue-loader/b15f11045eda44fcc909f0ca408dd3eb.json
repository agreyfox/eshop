reventDefault()\n      let rules = [ ...this.rules ]\n      rules.splice(index, 1)\n      this.$emit('update:rules', [ ...rules ])\n    },\n    create (event) {\n      event.preventDefault()\n\n      this.$emit('update:rules', [\n        ...this.rules,\n        {\n          allow: true,\n          path: '',\n          regex: false,\n          regexp: {\n            raw: ''\n          }\n        }\n      ])\n    }\n  }\n}\n</script>\n"]}]}